{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/about/?$",
      "dest": "/about.html"
    },
    {
      "src": "^/products/?$",
      "dest": "/products.html"
    },
    {
      "src": "^/contact/?$",
      "dest": "/contact.html"
    },
    {
      "src": "^/cart/?$",
      "dest": "/addToCart.html"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "^/(.*\\.(css|js|json|png|jpg|jpeg|gif|svg|ico))$",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.css)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*\\.js)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
